  <% if @user.videos.count > 0%>
    <% @video_info.each do |info| %>
      <h3><%= info.title %></h3>
      <div class="youtube-container">        
        <iframe src="https://www.youtube.com/embed/<%= create_video_id info.url %>" width="560" height="315" frameborder="0" allowfullscreen="allowfullscreen"></iframe>
      </div>
      <p>PV: <%= info.pv%></p>
      <p><%= info.description%></p>
    <%end%>
  <%end%>